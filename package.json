{
  "name": "@tholu/nuxt-dayjs-module",
  "version": "2.0.0",
  "private": false,
  "description": "Forked from @nathanchase/nuxt-dayjs-module for dependency updates. This is a Nuxt 3 compatible module built with the official Nuxt 3 (module-builder)(https://github.com/nuxt/module-builder) for (dayjs)(https://github.com/iamkun/dayjs/).",
  "keywords": [
    "dayjs",
    "nuxt",
    "nuxt3"
  ],
  "homepage": "https://github.com/tholu/dayjs#readme",
  "bugs": {
    "url": "https://github.com/tholu/dayjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tholu/dayjs.git"
  },
  "license": "MIT",
  "author": "Thomas Lutz",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "prepack": "nuxt-module-build"
  },
  "dependencies": {
    "@nuxt/kit": "3.13.2",
    "dayjs": "^1.11.13"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/node": "22.7.5",
    "eslint": "latest",
    "nuxt": "^3.13.2"
  }
}